	top:0;left:0;
	background:#fff url(img/bg_left.gif) repeat-x;
	background-position:top;
	z-index:-1;
}

#page-wrap { 
	min-width: 980px;
	max-width: 1460px;
	margin: 0px auto 0px 0px;
	width:expression(document.body.clientWidth < 982? "980px" : document.body.clientWidth > 1462? "1460px" : "auto");
	}

.page_courrier #page-wrap, .section49 #page-wrap { 
	min-width: 1600px;
	max-width: 1600px;
	margin: 0px auto 0px 0px;
	width:expression(document.body.clientWidth < 1600? "1600px" : document.body.clientWidth > 1600? "1600px" : "auto");
	}
	
.page_courrier #page-wrap #content div, .section49  #page-wrap #content div,.section49  #page-wrap #content h2,.page_courrier  #page-wrap #content h2 {
 margin-left: 0;margin-right:0}

