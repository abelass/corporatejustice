/* -- CSS definition for ECCJ --- */


* {
	margin: 0;
	padding: 0;
}
.on {
color: #525759;
	
}

body {
	margin: 0;
	padding: 0;
	font-size: 90%;	
	background:#fff url(img/bg_right_intranet.gif) repeat-x;
	background-position:top;
	color:#525759;
	font-family: Arial, Helvetica, Sans serif, Verdana;	
}


#topLeft {
	width:120px;
	height:260px;
